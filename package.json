  "devDependencies": {
    "nodemon": "*",
    "standard": "*"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "database-service": "file:local_modules/database-service",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "sqlite3": "^4.1.1"
  }
}
