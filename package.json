{
  "name": "sensibus",
  "version": "0.0.1",
  "description": "Time series logger and analyzer",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/axelpale/sensibus.git"
  },
  "scripts": {
    "build": "webpack --config client/webpack.config.js",
    "lint": "(cd timeline; npm run lint) && (cd site; npm run lint)",
    "test": "npm run lint",
    "lintserver": "standard \"server/**/*.js\" && echo OK",
    "start": "node server/index.js",
    "watchstart": "nodemon --watch server --ext js,json --exec \"npm run lintserver && npm start\"",
    "watchreact": "webpack -d --watch --config site/webpack.config.js"
  },
  "author": {
    "name": "Akseli Pal√©n",
