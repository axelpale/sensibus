{
  "name": "sensibus",
  "version": "0.0.1",
  "description": "Time series logger and analyzer",
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/axelpale/sensibus.git"
  },
  "scripts": {
    "build": "webpack --config client/webpack.config.js",
    "deploy": "rsync -r --include-from=\".deployinclude\" --exclude=\"*\" ./ xeli@xeli.webfactional.com:~/webapps/demos/sensibus",
    "watchbuild": "nodemon --watch client/src --watch local_modules --watch datasets --ext js,ejs,css,json --ignore version.js --exec \"npm run lint && npm run gv && npm run build\"",
    "lint": "standard \"client/src/**/*.js\" \"server/**/*.js\" && echo OK",
    "lintfix": "standard --fix \"client/src/**/*.js\" && echo OK",
    "test": "npm run lint",
    "gv": "genversion client/src/version.js",
    "lintserver": "standard \"server/**/*.js\" && echo OK",
    "start": "node server/index.js",
    "watchstart": "nodemon --watch server --ext js,json --exec \"npm run lintserver && npm start\""
  },
  "author": {
    "name": "Akseli Pal√©n",
    "email": "akseli.palen@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "css-loader": "^3.2.0",
    "ejs-loader": "^0.3.3",
    "genversion": "^2.2.0",
    "nodemon": "*",
    "standard": "*",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "deep-equal": "^1.0.1",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "problib": "file:./local_modules/problib",
    "redux": "^4.0.5",
    "reselect": "^4.0.0",
    "senseway": "file:./local_modules/senseway",
    "sqlite3": "^4.1.1",
    "uilib": "file:local_modules/uilib"
  }
}
